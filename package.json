{
  "name": "react-redux-example",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "author": "Nick Staroba",
  "scripts": {
    "build": "npx --ignore-existing --quiet rimraf dist/* && webpack --mode production",
    "build:watch": "webpack --mode production --watch",
    "clean:build": "npx --ignore-existing --quiet rimraf dist/**/*",
    "clean:build-deps": "npx --ignore-existing --quiet rimraf dist/node_modules",
    "clean:deps": "npx --ignore-existing --quiet rimraf package-lock.json node_modules",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint \"**/*.{js,jsx}\" --fix",
    "lint:prettier": "prettier \"./**/*.{css,html,js,json,jsx,md,scss,yaml}\" --write",
    "lint:stylelint": "stylelint \"client/**/*.{css,scss}\"",
    "release": "standard-version",
    "start:api": "nodemon api/server.js",
    "start:client": "webpack-dev-server",
    "test": "jest --maxWorkers 1",
    "test:changed": "jest --maxWorkers 1 --onlyChanged",
    "test:coverage": "jest --maxWorkers 1 --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:changed",
      "prepare-commit-msg": "git-hooks/prepare-commit-msg.sh ${HUSKY_GIT_PARAMS}",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@proto-component/react-library-a": "0.1.2",
    "json-server": "0.16.1",
    "lorem-ipsum": "2.0.3",
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@babel/preset-env": "7.10.1",
    "@babel/preset-react": "7.10.1",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@eterna/configurator": "0.1.3",
    "@testing-library/jest-dom": "5.8.0",
    "@testing-library/react": "10.0.4",
    "@testing-library/react-hooks": "3.2.1",
    "autoprefixer": "9.8.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "copy-webpack-plugin": "6.0.1",
    "css-loader": "3.5.3",
    "eslint": "7.1.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.6",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.5",
    "raw-loader": "4.0.1",
    "redux-devtools": "3.5.0",
    "sass-loader": "8.0.2",
    "sass-resources-loader": "2.0.3",
    "standard-version": "8.0.0",
    "style-loader": "1.2.1",
    "stylelint": "13.5.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-sass-guidelines": "7.0.0",
    "svg-react-loader": "0.4.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  }
}
